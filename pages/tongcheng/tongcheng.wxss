/* pages/tongcheng/tongcheng.wxss */
body{
  background-color: #F2F2F2;
}
.zepto{
  width: 750rpx;
  height: 266rpx;
}
/*首页轮播图  */
.body{
  width: 100%;
  height: auto;
  background: orange;
}
.banner{
  width: 100%;
  height: 360rpx;
}
/*轮播  */
.swiper-container{
 position: relative;
}
.swiper-container .swiper{
 height: 360rpx;
}
.swiper-container .swiper .img{
 width: 100%;
 height: 100%;
}
.dots{
 position: absolute;
 left: 0;
 right: 0;
 bottom: 20rpx;
 display: flex;
 /* background: #000; */
 width: 238rpx;
 margin: auto;
 border-radius: 10rpx;
 height: 20rpx;
 justify-content: center;
}
.dot{
 margin: 4rpx 8rpx;
 width: 30rpx;
 height: 6rpx;
 background: rgb(255, 255, 255);
 border:rgb(218, 69, 70) solid 1rpx;
 border-radius: 8rpx;
 transition: all .6s;
}
.active{
 width: 30rpx;
 background: #f80;
}
/* 天气模块 */
.weathernav{
  width: 750rpx;
  height: 155rpx;
  background-color: white;
  margin-top: 20rpx;
}
.weathericon{
width: 57rpx;
height: 57rpx;
 margin-top: 50rpx;  
margin-left: 44rpx;
display: inline-block;
}
/* 天气文字信息 */
.weather{
  margin-left: 32rpx;
   /* margin-top: 50rpx;  */
  display: inline-block;
}
.weatherinfo{
  font-size: 26rpx;
  height: 26rpx;
}
.weathertime{
  margin-top: 8rpx;
  font-size: 26rpx;
  height: 26rpx;
}
/* 签到button */
.weathernav>button{
background-color:#45C018;
display: inline-block;
margin-left: 118rpx;
font-size: 26rpx;
color: rgb(255, 255, 255);
border: rgb(63, 176, 21) solid 1rpx;
border-radius: 3px;
line-height: 50rpx;
width: 142rpx;
padding-left: 0rpx;
padding-right: 0rpx;
}
/* 导航模块 */
.nav{
  display: flex;
  flex-wrap: wrap;
  background-color: white;  
}
.nav-mode{
  height: 134rpx;
  width: 105rpx;
  overflow: hidden;
  font-size: 26rpx;
  text-align: center;
  color:rgb(0, 0, 0);
   margin: 20rpx 40rpx;
}
.nav-mode>image{
  display: inline-block;
  width: 94rpx;
  height: 94rpx;
}
.nav-name{
  display: inline-block;
  color: black;
  width: 100%;
  height: 10rpx;
  font-size: 6rpx;
}
/* 导航模块开始 */
.ad{
  margin-top: 24rpx;
  margin-bottom: 24rpx;
  height: 64rpx;
  /* background-color: greenyellow; */
  text-align: top;
  background-color: white;
}
.ad-icon{
  line-height: 64rpx;
  margin-top: 15rpx;
  margin-left: 30rpx;
  float: left;
  height: 35rpx;
  width: 40rpx;
}
.adinfo{
  margin-left: 32rpx;
  float: left;
  font-size: 26rpx;
  margin-top: 13rpx;
}
/* 导航模块结束 */
/* 商家入驻开始 */
.market{
  width: 100%;
  height: 340rpx;
  background-color: white;
  overflow-x: hidden; 
}
.markettitle{
  width: 686rpx;
  height: 63rpx;
  background-color: white;  
  line-height: 63rpx;
  margin-left: 32rpx;
  font-size: 26rpx;
  color: rgb(0, 0, 0);
  border-bottom: rgb(242, 242, 242) solid 1px;
}
.moremarket{
  /* display: inline-block; */
  float: right;
  color:rgb(102, 102, 102);
  font-size: 24rpx;
  overflow-x: hidden;
}
.marketlist{
  padding-left: 30rpx;
  padding-bottom: 30rpx;
  padding-top: 30rpx;
  /* overflow-x: scroll; */
  /* width: 150%; */
  height: 210rpx;
  /* background-color: powderblue; */
}
.marketlist_one{
  width: 150rpx;
  height: 200rpx;
  /* background-color: pink; */
  border-radius: 6rpx;
  margin-right: 30rpx;
  float: left;
}
.marketlist_wai{
  width: 150%;
  height: 200rpx;
  /* background-color: yellow; */
}
.marketlist_one>image{
  width: 150rpx;
  height: 150rpx;
}
.marketlist_one>view{
  font-size: 26rpx;
  color: #333;
  text-align: center;
  margin-top: 10rpx;
}
/* 商家入驻结束 */
/* 二级导航开始 */
.nav-two{
  height: 88rpx;
  margin-top: 14rpx;
  /* margin-bottom: 10rpx; */
  background-color: white;
  /* margin-left: 90rpx; */
}
.uls{
  height: 88rpx;
  width: 150%;
}
.listss{
  padding:14rpx 32rpx 0 32rpx;
  height: 83rpx;
  float :left;
  line-height: 88rpx;
  font-size: 26rpx;
  color: rgb(102, 102, 102);
  border-bottom: 5rpx solid #FF8F0B
}
.lists{
  height: 88rpx;
  float :left;
  line-height: 88rpx;
  font-size: 26rpx;
  color: rgb(102, 102, 102);
  margin-right: 58rpx;
}
/* 二级导航结束 */
.markets{
  width: 100%;
  margin-bottom: 24rpx;
  background-color: white;
}
.marketinfo{
  width: 690rpx;
  margin-left: 30rpx;
  margin-top: 23rpx;
  position: relative;

}
.market-logo{
  width: 104rpx;
  height: 104rpx;
  display: inline-block;
}
.market-title{
 position: absolute;
 top: 0;
 left: 90rpx;
 margin-top: 10rpx;
 vertical-align: top;
  margin-left: 26rpx; 
 color: rgb(30 , 173, 240);
 font-size: 30rpx;
}
.top{
  position: absolute;
  top: 60rpx;
  left: 120rpx;
  height: 44rpx;
  padding:0rpx 20rpx;
  line-height: 44rpx;
  font-size: 28rpx;
  background-color: #FF8F0B;
 color: white;
}
.serve{
  position: absolute;
  top: 60rpx;
  left:200rpx;
  height: 44rpx;
  padding:0rpx 20rpx;
  line-height: 44rpx;
  font-size: 28rpx; 
  background-color: #1CDBA7;
  color: white;
}
.tel{
  width: 204rpx;
  height: 58rpx;
  float: right;
  vertical-align: center;
  margin-top: 22rpx;
  border-radius: 10rpx;
}
.marketintro{
  /* background-color: greenyellow; */
  padding:0 30rpx;
  padding-top: 30rpx;
  padding-bottom: 26rpx;
  color: rgb(102, 102, 102);
  font-size: 26rpx;
  line-height: 48rpx;
}
.marketimg{
  height: 158rpx;
  padding: 0 30rpx;
  display: flex;
}
.marketimg>image{
  width: 158rpx;
  height: 158rpx;
  margin-right: 20rpx;
}
.view{
  padding: 0 30rpx;
  height:143rpx;
  position: relative;
}
.viewtime{
  float: left;
  font-size: 24rpx;
  color: rgb(102, 102, 102);
  margin-top: 22rpx;
}
.more{
  /* display: inline-block; */
  font-size: 24rpx;
  color: rgb(39, 122, 161);
  float: right;
  margin-top: 22rpx;
}
.viewnum{
  background-color: #F1F1F1;
  width: 100%;
  height: 54rpx;
  margin-top: 16rpx;
  float: left;
  line-height: 44rpx;
  color: #727272;
  font-size: 24rpx;
}
.kuang{
  position:absolute;
  content:"";
  border-width:15rpx;
  border-style:solid;
  border-color:transparent transparent #F1F1F1 #F1F1F1;
  transform:rotate(135deg);
  left:60rpx;
  top:60rpx;
}
.views{
  margin-left: 18rpx;
  display: inline-block;
  width: 37rpx;
  height: 20rpx;
}
.viewsnum{
  margin-left: 20rpx;
  display: inline-block;
  height: 20rpx;
  margin-right: 7rpx;
  color: #2CB2F0;
  font-size: 24rpx;
}
.zan{
  margin-left: 38rpx;
  display: inline-block;
  width: 33rpx;
  height: 33rpx;
  margin-top:10rpx;
}
.zannum{
  margin-left: 20rpx;
  display: inline-block;
  height: 20rpx;
  margin-right: 7rpx;
  color: #2CB2F0;
  font-size: 24rpx;
}
.clearfix {
  overflow: auto;
  _height: 1%;
}
/* 底部导航栏开始 */
.tab-space{
  height: 98rpx;
  width: 100%;
}
.tab{
  height: 98rpx;
  width: 100%;
  background-color: white;
  display: flex;
  position:fixed;
  bottom:0px;
}
.tab>view{
width: 15%;
/* background-color: greenyellow; */
margin-left: 4%;
text-align: center;
}
.tab image{
  width: 34rpx;
  height: 34rpx;
  margin-top: 16rpx;
}
.tab>view{
  font-size: 24rpx;
  color: rgb(102, 102, 102);
}